<core:FragmentDefinition xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns:core="sap.ui.core" xmlns="sap.m">
	<Dialog class="sapUiPopupWithPadding" contentWidth="auto" title="Delete Photo">
		<content>
			<Text text="Are you sure you want to delete this photo?"/>
		</content>
		<beginButton>
			<Button app:dialogType="FileRemover" id="okRemoveDialogButton" press="onRemoveDialogCloseButton" text="Ok"/>
		</beginButton>
		<endButton>
			<Button app:dialogType="FileRemover" press="onCancelButtonPress" text="Cancel"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>